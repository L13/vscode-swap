{
	"name": "l13-swap",
	"displayName": "L13 Swap",
	"description": "Invert or rotate keywords in JavaScript and TypeScript",
	"version": "0.14.0",
	"publisher": "L13RARY",
	"license": "SEE LICENCE IN LICENCE.md",
	"preview": true,
	"homepage": "https://github.com/l13/vscode-swap/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/l13/vscode-swap.git"
	},
	"bugs": {
		"url": "https://github.com/l13/vscode-swap/issues"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#232323",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"swap",
		"invert",
		"rotate",
		"keyword"
	],
	"activationEvents": [
		"onCommand:l13Swap.invert",
		"onCommand:l13Swap.rotate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "l13Swap.invert",
				"title": "Invert Keyword"
			},
			{
				"command": "l13Swap.rotate",
				"title": "Rotate Keyword"
			}
		],
		"keybindings": [
			{
				"mac": "meta+i",
				"key": "ctrl+i",
				"command": "l13Swap.invert",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"mac": "meta+shift+i",
				"key": "ctrl+shift+i",
				"command": "l13Swap.rotate",
				"when": "editorTextFocus && !editorReadonly"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "gulp build",
		"watch": "gulp watch",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.14.12",
		"del": "^4.1.1",
		"gulp": "^4.0.2",
		"rollup": "^1.16.6",
		"rollup-plugin-typescript": "^1.0.1",
		"tslint": "^5.18.0",
		"typescript": "^3.4.5",
		"vscode": "^1.1.35"
	}
}
