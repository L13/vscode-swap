{
	"name": "l13-swap",
	"displayName": "Swap Keywords",
	"description": "Invert or rotate keywords in JavaScript and TypeScript",
	"version": "0.16.0",
	"publisher": "L13RARY",
	"license": "SEE LICENCE IN LICENCE.md",
	"homepage": "https://github.com/l13/vscode-swap/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/l13/vscode-swap.git"
	},
	"bugs": {
		"url": "https://github.com/l13/vscode-swap/issues"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#232323",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"swap",
		"invert",
		"rotate",
		"keyword"
	],
	"activationEvents": [
		"onCommand:l13Swap.action.keyword.invert",
		"onCommand:l13Swap.action.keyword.rotate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "l13Swap.action.keyword.invert",
				"title": "Invert Keyword"
			},
			{
				"command": "l13Swap.action.keyword.rotate",
				"title": "Rotate Keyword"
			}
		],
		"keybindings": [
			{
				"mac": "meta+i",
				"key": "ctrl+i",
				"command": "l13Swap.action.keyword.invert",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"mac": "meta+shift+i",
				"key": "ctrl+shift+i",
				"command": "l13Swap.action.keyword.rotate",
				"when": "editorTextFocus && !editorReadonly"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "gulp build",
		"watch": "gulp watch",
		"test": "npm run compile"
	},
	"devDependencies": {
		"@rollup/plugin-typescript": "^5.0.2",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^12.12.35",
		"@types/vscode": "^1.43.0",
		"del": "^5.1.0",
		"glob": "^7.1.3",
		"gulp": "^4.0.2",
		"mocha": "^7.1.1",
		"rollup": "^2.22.2",
		"tslint": "^6.1.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.0.2"
	}
}
